        .card-projects.cursor-pointer.animate-card
          a(href="/projects") 
            .card-img-wrapper.pro-img-wrapper.animate-card-left
              img.card-img(src='https://images.unsplash.com/photo-1600607686527-6fb886090705?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8Mnx8aW50ZXJpb3JzfGVufDB8MXwwfHx8MA%3D%3D&auto=format&fit=crop&w=500&q=60', alt='')
              p.card-over-text.overlay-text.card-over-pro Projects
        // Category Card
        .card-category.cursor-pointer
          .card-img-wrapper.cat-img-wrapper.animate-card-right
            .div-multi-img
              a(href="/categories")
                // <img class='card-img' src='https://images.unsplash.com/photo-1600607686527-6fb886090705?ixlib=rb-4.0.3&ixid=M3wxMjA3fDB8MHxzZWFyY2h8Mnx8aW50ZXJpb3JzfGVufDB8MXwwfHx8MA%3D%3D&auto=format&fit=crop&w=500&q=60' alt=''>
