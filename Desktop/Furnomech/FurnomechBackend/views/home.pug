extends base

block files 
  link(rel='stylesheet', href='css/home.css') 
  link(rel="shortcut icon", href="data/icons/favicon.png", type="image/x-icon")
  script(defer, src='js/home.js')

block content 
    // **********************************************************************************
    // Section Home
    // **********************************************************************************
    section.section.section-home.contanier
      .div-home-text
        .div-title-wrapper 
          h1.animate-title.home-title Elegant
          h1.animate-title.home-title  interiors  
          // <h1 class='animate-title home-title'> & custom-built  </h1>
          h1.animate-title.home-title &amp; modular
          h1.animate-title.home-title furniture
        .home-btns
          a.primary-link.animate-link.home-about-us-link(href='/contact-us') GET IN TOUCH
      .div-home-img
        .div-pri-img
          img.animate-img.pri-img(src='https://assets.website-files.com/62da9129694c950b9ffca6a0/62e6b44b069cdfd7f28f7ea9_1500x1920-01.jpg', alt='')
        .div-sec-img.div-sec-img-left.move-img
          img.animate-img.sec-img(src='https://assets.website-files.com/62da9129694c950b9ffca6a0/62f263b65a19e15d3eab4289_img-header-01.jpg', alt='')

    // **********************************************************************************
    // Extended Div About-Us
    // **********************************************************************************
    .about-content.animate-element
      .about-text
        h1.about-para
          span.section-type.animate-title.div-about-us-inline
            sup.section-type.about-us-inline about us
          span.italic &nbsp;&nbsp;&nbsp; &nbsp;&nbsp;&nbsp;&nbsp;&nbsp; Immerse
          |  yourself in unparalleled craftsmanship, meticulous attention to 
          span.italic detail
          | , and unwavering dedication to 
          span.italic excellence
          | . Welcome to 
          strong
            span.italic Furnomech
          | , where visions 
          span.italic transcend
          |  imagination.
        // Immerse yourself in unparalleled craftsmanship, meticulous attention to detail, and unwavering dedication to excellence. Welcome to Furnomech, where visions transcend imagination.
      .about-btn-container
        a.secondary-link.about-btn.animate-link(href='/about-us') Learn more
    
    // **********************************************************************************
    // Section Features
    // **********************************************************************************
    section.section.section--features
      .div-section-title
        p.section-type.animate-title features
        p.section-title.animate-title
          span.italic We do
          |  it best.
      .section-content.animate-element.features-content
        .div-btn-scroll
          ion-icon.arrow.arrow-left(name='chevron-back-outline')
          .div-features-btn
            // Buttons Markup
          ion-icon.arrow.arrow-right(name='chevron-forward-outline')
        .div-features-desc
